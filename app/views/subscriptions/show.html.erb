<table>
	<tr>
		<th>Product</th>
		<th>Quantity</th>
		<th>Price</th>
  </tr>
  <% @subscription.subscription_lines.each do |line| %>
    <tr>
    	<td><%= line.product.name %></td>
    	<td><%= line.quantwity %></td>
    	<td><%= line.price * line.quantity %></td>
    </tr>
   <% end %>
  <tr>
  	<td colspan=2>Total</td>
  	<td><%= @subscription.total %></td>
 </tr>
</table>